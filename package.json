{
  "version": "0.2.0",
  "name": "carvago-frontend-assignment",
  "description": "Assignment in React.js for developers who want to join the Carvago FE team.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Carvago/frontend-assignment"
  },
  "scripts": {
    "start": "concurrently \"npm run start-react\" \"npm run start-server\"",
    "start-react": "react-scripts start",
    "start-server": "ts-node backend/server.ts",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "prettify": "prettier --write --config ./.prettierrc --ignore-path ./.prettierignore .",
    "eslint": "eslint --cache --fix -c ./.eslintrc.json .",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "generate-types": "openapi-typescript backend/swagger/openApi.json --output types/api.ts"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "2.5.5",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@reduxjs/toolkit": "^1.9.3",
    "@tanstack/react-query": "^5.85.6",
    "@tanstack/react-query-devtools": "^5.85.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "cypress": "^13.5.0",
    "dotenv": "^16.4.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.2",
    "framer-motion": "^10.10.0",
    "i18next": "^21.5.2",
    "i18next-browser-languagedetector": "^6.1.2",
    "jsonwebtoken": "^9.0.2",
    "nedb": "^1.8.0",
    "prettier": "^2.7.1",
    "ramda": "^0.29.1",
    "ramda-adjunct": "^4.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "1.3.0",
    "react-hook-form": "^7.62.0",
    "react-i18next": "12.2.0",
    "react-router": "^7.8.2",
    "react-scripts": "^5.0.1",
    "styled-components": "^6.1.1",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.2",
    "ts-pattern": "^5.0.6",
    "typescript": "5.3.3",
    "uuid": "^9.0.1",
    "web-vitals": "3.3.0",
    "yup": "^1.3.3",
    "zustand": "^5.0.8"
  },
  "overrides": {
    "typescript": "5.3.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.3",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/nedb": "^1.8.16",
    "@types/node": "^18.15.11",
    "@types/ramda": "^0.29.9",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.0.11",
    "@types/react-router": "^5.1.2",
    "@types/styled-components": "^5.1.26",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "openapi-typescript": "^7.9.1"
  }
}
